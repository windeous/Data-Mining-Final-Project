    "import pandas as pd\n",
    "\n",
    "# create a sample dataframe\n",
    "df = pd.DataFrame({'A': [1, 2, 1], 'B': [2, 2, 3], 'C': [3, 3, 3], 'D': [4, 5, 5]})\n",
    "\n",
    "# drop columns where all values are the same\n",
    "df = df.loc[:, (df != df.iloc[0]).any()]\n",
    "\n",
    "# view the dataframe after dropping columns\n",
    "print(df)"
